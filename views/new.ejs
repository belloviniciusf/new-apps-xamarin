<%- include top.ejs %>
<form action="<%= action %>" method="POST" enctype="multipart/form-data">
    <div class="form-group">
        <label>Planilha a ser convertida*:</label>
        <input action='file/upload' type="file" class="form-control" 
            name="file"
            ngf-select ngf-pattern="'.xls,.csv,.xlsx'" accept=".xls,.csv,.xlsx" ngf-max-files="1">
    </div>
    <input type="submit" value="Converter" class="btn btn-primary" /><a href="/" class="btn">Cancelar</a>
</form>
<%- include bottom.ejs %>