<%- include top.ejs %>
<form action="<%= action %>" enctype="multipart/form-data" method="POST">
    <div class="form-group">
        <label>Name of new app*:</label>
        <input type="text" class="form-control" 
            name="name">
        <label>Icon and logo of new app*:</label>
        <input type="file" class="form-control" 
            name="files" multiple>        
    </div>
    <input type="submit" value="Create" class="btn btn-primary" /><a href="/" class="btn">Cancel</a>    
</form>
<%- include bottom.ejs %>